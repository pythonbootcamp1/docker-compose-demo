# Frontend (React + Vite) Environment Variables
# Copy to .env.dev or .env.prod and fill in values

# API URLs
# Development: Use direct service URLs
# Production: Use relative paths for Nginx reverse proxy
VITE_AUTH_API_URL=http://localhost:8000
VITE_BLOG_API_URL=http://localhost:8001

# App Configuration (optional)
VITE_APP_NAME=United Server Blog
VITE_APP_VERSION=1.0.0
